{
  "id": 114,
  "name": "الناس",
  "transliteration": "An-Nas",
  "translation": "Mankind",
  "type": "meccan",
  "total_verses": 6,
  "verses": [
    {
      "id": 1,
      "text": "قُلۡ أَعُوذُ بِرَبِّ ٱلنَّاسِ",
      "translation": "(ऐ नबी!) कह दीजिए : मैं शरण लेता हूँ लोगों के पालनहार की।",
      "transliteration": "Qul aAAoothu birabbi alnnasi"
    },
    {
      "id": 2,
      "text": "مَلِكِ ٱلنَّاسِ",
      "translation": "लोगों के बादशाह की।",
      "transliteration": "Maliki alnnasi"
    },
    {
      "id": 3,
      "text": "إِلَٰهِ ٱلنَّاسِ",
      "translation": "लोगों के सत्य पूज्य की।",
      "transliteration": "Ilahi alnnasi"
    },
    {
      "id": 4,
      "text": "مِن شَرِّ ٱلۡوَسۡوَاسِ ٱلۡخَنَّاسِ",
      "translation": "वसवसा डालने वाले, पीछे हट जाने वाले की बुराई से।",
      "transliteration": "Min sharri alwaswasi alkhannasi"
    },
    {
      "id": 5,
      "text": "ٱلَّذِي يُوَسۡوِسُ فِي صُدُورِ ٱلنَّاسِ",
      "translation": "जो लोगों के दिलों में वसवसे डालता है।",
      "transliteration": "Allathee yuwaswisu fee sudoori alnnasi"
    },
    {
      "id": 6,
      "text": "مِنَ ٱلۡجِنَّةِ وَٱلنَّاسِ",
      "translation": "जिन्नों और इनसानों में से।",
      "transliteration": "Mina aljinnati wa alnnasm"
    }
  ]
}